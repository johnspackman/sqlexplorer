<fragment>
   <extension
         point="net.sourceforge.sqlexplorer.nodeDetailTab">
      <detailtab
            class="net.sourceforge.sqlexplorer.informix.tabs.ProcessTab"
            database-product-name="Informix*"
            id="net.sourceforge.sqlexplorer.informix.processTab"
            name="Process List"
            node-type="database">
      </detailtab>
      <detailtab
            class="net.sourceforge.sqlexplorer.informix.tabs.ProcedureTab"
            database-product-name="Informix*"
            id="net.sourceforge.sqlexplorer.informix.ProcedureTab"
            name="Procedure Information"
            node-type="procedure,package_procedure">
      </detailtab>
      <detailtab
            class="net.sourceforge.sqlexplorer.informix.tabs.FunctionTab"
            database-product-name="Informix*"
            id="net.sourceforge.sqlexplorer.informix.FunctionTab"
            name="Function Information"
            node-type="function,function_procedure">
      </detailtab>
      <detailtab
            class="net.sourceforge.sqlexplorer.informix.tabs.SpaceTab"
            database-product-name="Informix*"
            id="net.sourceforge.sqlexplorer.informix.SpaceTab"
            name="Space Information"
            node-type="SpaceNode">
      </detailtab>
      <detailtab
            class="net.sourceforge.sqlexplorer.informix.tabs.ChunkTab"
            database-product-name="Informix*"
