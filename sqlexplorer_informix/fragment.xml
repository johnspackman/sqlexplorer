<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="net.sourceforge.sqlexplorer.nodeDetailTab">
      <detailtab
            class="net.sourceforge.sqlexplorer.informix.tabs.ProcessTab"
            database-product-name="Informix*"
            id="net.sourceforge.sqlexplorer.informix.processTab"
            name="Process List"
            node-type="database">
      </detailtab>
      <detailtab
            class="net.sourceforge.sqlexplorer.informix.tabs.ProcedureTab"
            database-product-name="Informix*"
            id="net.sourceforge.sqlexplorer.informix.ProcedureTab"
            name="Procedure Information"
            node-type="procedure,package_procedure">
      </detailtab>
      <detailtab
            class="net.sourceforge.sqlexplorer.informix.tabs.FunctionTab"
            database-product-name="Informix*"
            id="net.sourceforge.sqlexplorer.informix.FunctionTab"
            name="Function Information"
            node-type="function,function_procedure">
      </detailtab>
   </extension>
   <extension
         point="net.sourceforge.sqlexplorer.node">
      <node
            class="net.sourceforge.sqlexplorer.informix.nodes.ProcedureFolder"
            database-product-name="Informix*"
            id="net.sourceforge.sqlexplorer.informix.nodes.ProcedureFolder"
            name="Procedure Folder"
            parent-node="schema+catalog"
            table-type="procedure_folder">
      </node>
      <node
            class="net.sourceforge.sqlexplorer.informix.nodes.FunctionFolder"
            database-product-name="Informix*"
            id="net.sourceforge.sqlexplorer.informix.nodes.FunctionFolder"
            name="Function Folder"
            parent-node="schema+catalog"
            table-type="function_folder">
      </node>
   </extension>

</fragment>
